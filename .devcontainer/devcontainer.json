{
  "name": "moshimoshi-web",
  "dockerComposeFile": ["../docker-compose.yml"],
  "service": "web",
  "workspaceFolder": "/home/alice/web",
  "postAttachCommand": "git config --global core.pager \"less -FX\" && cp -f /home/alice/web/.devcontainer/pre-commit /home/alice/web/.git/hooks/",
  "shutdownAction": "stopCompose",
  "initializeCommand": "ls",
  "containerUser": "alice",
  "customizations": {
    "vscode": {
      "extensions": [
        "denoland.vscode-deno",
        "wmaurer.change-case",
        "sleistner.vscode-fileutils",
        "usernamehw.errorlens",
        "albymor.increment-selection",
        "huizhou.githd",
        "GitHub.github-vscode-theme",
        "naumovs.color-highlight",
        "GitHub.copilot"
      ],
      "settings": {
        "deno.enablePaths": ["/home/alice/web"],
        "deno.enable": true,
        "editor.formatOnSave": true,
        "editor.formatOnSaveMode": "file",
        "files.insertFinalNewline": true,
        "terminal.integrated.defaultProfile.linux": "bash",
        "terminal.integrated.profiles.linux": {
          "bash": { "path": "/bin/bash" }
        },
        "editor.defaultFormatter": "denoland.vscode-deno",
        "workbench.colorTheme": "GitHub Dark Colorblind"
      }
    }
  }
}
